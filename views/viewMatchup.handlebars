<h1>Matchup Details</h1>

<h2>{{#each matchup}}{{this.homeTeamName}} {{this.homeTeamPoints}} - {{this.awayTeamPoints}} {{this.awayTeamName}}{{/each}}</h2>

<h3>{{#each matchup}}{{this.homeTeamName}}{{/each}}</h3>
<table>
    <tr>
        <th>Player</th>
        <th>Date</th>
        <th>FGM</th>
        <th>FGA</th>
        <th>FTM</th>
        <th>FTA</th>
        <th>Points</th>
        <th>Assists</th>
        <th>Rebounds</th>
        <th>Steals</th>
        <th>Blocks</th>
        <th>Turnovers</th>
        <th>Fantasy Points</th>
    </tr>
{{#each homePerformance}}
    <tr>
        <td>{{this.fName}} {{this.lname}}</td>
        <td>{{this.date}}</td>
        <td>{{this.FGM}}</td>
        <td>{{this.FGA}}</td>
        <td>{{this.FTM}}</td>
        <td>{{this.FTA}}</td>
        <td>{{this.points}}</td>
        <td>{{this.assists}}</td>
        <td>{{this.rebounds}}</td>
        <td>{{this.steals}}</td>
        <td>{{this.blocks}}</td>
        <td>{{this.turnovers}}</td>
        <td>{{this.FantasyPoints}}</td>
    </tr>
{{/each}}
</table>

<h3>{{#each matchup}}{{this.awayTeamName}}{{/each}}</h3>
<table>
    <tr>
        <th>PlayerId</th>
        <th>Date</th>
        <th>FGM</th>
        <th>FGA</th>
        <th>FTM</th>
        <th>FTA</th>
        <th>Points</th>
        <th>Assists</th>
        <th>Rebounds</th>
        <th>Steals</th>
        <th>Blocks</th>
        <th>Turnovers</th>
        <th>Fantasy Points</th>
    </tr>
{{#each awayPerformance}}
    <tr>
        <td>{{this.fName}} {{this.lname}}</td>
        <td>{{this.date}}</td>
        <td>{{this.FGM}}</td>
        <td>{{this.FGA}}</td>
        <td>{{this.FTM}}</td>
        <td>{{this.FTA}}</td>
        <td>{{this.points}}</td>
        <td>{{this.assists}}</td>
        <td>{{this.rebounds}}</td>
        <td>{{this.steals}}</td>
        <td>{{this.blocks}}</td>
        <td>{{this.turnovers}}</td>
        <td>{{this.FantasyPoints}}</td>
    </tr>
{{/each}}
</table>