<h1>All Players</h1>

<table id="players">
    <tr>
        <th>Name</th>
        <th>Team</th>
        <th>Position</th>
        <th>FGM</th>
        <th>FGA</th>
        <th>FTM</th>
        <th>FTA</th>
        <th>Points</th>
        <th>Assists</th>
        <th>Rebounds</th>
        <th>Steals</th>
        <th>Blocks</th>
        <th>Turnovers</th>
        <th>Fantasy Points</th>
        <th>Team</th>
    </tr>
{{#each player}}
    <tr>
        <td>{{this.fName}} {{this.lname}}</td>
        <td>{{this.basketballTeam}}</td>
        <td>{{this.position}}</td>
        <td>{{this.FGM}}</td>
        <td>{{this.FGA}}</td>
        <td>{{this.FTM}}</td>
        <td>{{this.FTA}}</td>
        <td>{{this.points}}</td>
        <td>{{this.assists}}</td>
        <td>{{this.rebounds}}</td>
        <td>{{this.steals}}</td>
        <td>{{this.blocks}}</td>
        <td>{{this.turnovers}}</td>
        <td>{{this.FantasyPoints}}</td>
        <td>{{this.abbreviation}}</td>
    </tr>
{{/each}}
</table>