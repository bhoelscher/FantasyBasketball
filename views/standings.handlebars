<h1>Standings</h1>

<table>
    <tr>
        <th>Team</th>
        <th>Wins</th>
        <th>Losses</th>
        <th>Ties</th>
    </tr>
{{#each standing}}
    <tr>
        <td>{{this.fantasyTeamName}}</td>
        <td>{{this.wins}}</td>
        <td>{{this.losses}}</td>
        <td>{{this.ties}}</td>
    </tr>
{{/each}}
</table>